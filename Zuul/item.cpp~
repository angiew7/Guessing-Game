/*
This is the class for items
 */ 
#include <iostream>
#include "item.h"
using namespace std;

item::item(){
}
//set item
item::item(char *newDesc){
  description = newDesc;
}
//get item
char* item::getItem(){
  char *a = (char*)malloc(50*sizeof(char));
  strcpy(a, description);
  return description;
}
